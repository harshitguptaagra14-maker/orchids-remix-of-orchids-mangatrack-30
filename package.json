{
  "name": "mangatrack",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "node scripts/check-react-versions.js || true",
    "predev": "node scripts/watchdog.js ensure",
    "dev": "bash ./scripts/start-dev-safe.sh",
    "dev:direct": "next dev --turbopack -H 0.0.0.0 -p 3000",
    "watchdog": "node scripts/watchdog.js",
    "watchdog:start": "node scripts/watchdog.js watch &",
    "dev:workers": "npx tsx watch src/workers/index.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:workers\"",
    "build": "node scripts/check-react-versions.js && prisma generate && next build",
    "postinstall": "node scripts/patch-supabase-ssr.js && prisma generate && node scripts/watchdog.js check || node -e \"const fs=require('fs');const path=require('path');const forbidden=['@number-flow/react','number-flow','@react-three/fiber','@react-three/drei','three','three-globe','@types/three','cobe'];forbidden.forEach(d=>{const p=path.join('node_modules',d);if(fs.existsSync(p)){fs.rmSync(p,{recursive:true,force:true});console.log('Removed phantom:',d)}})\"",
    "start": "next start",
    "start:prod": "node scripts/check-build.js && next start -p ${PORT:-3002}",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "test:load": "echo 'Run: k6 run load-tests/api-load-test.js'",
    "test:load:smoke": "echo 'Run: k6 run --vus 5 --duration 30s load-tests/api-load-test.js'",
    "audit": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix",
    "worker": "npx tsx src/workers/index.ts",
    "pm2:start": "npx pm2 start ecosystem.config.js",
    "pm2:stop": "npx pm2 stop ecosystem.config.js",
    "pm2:restart": "npx pm2 restart ecosystem.config.js",
    "pm2:reload": "npx pm2 reload ecosystem.config.js",
    "pm2:logs": "npx pm2 logs",
    "pm2:status": "npx pm2 status",
    "pm2:save": "npx pm2 save",
    "pm2:setup": "bash scripts/setup-pm2.sh",
    "deploy:prod": "npm run build && npm run db:migrate && npm run pm2:reload && npm run pm2:save",
    "db:migrate": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:seed": "prisma db seed",
    "check:react": "node scripts/check-react-versions.js"
  },
